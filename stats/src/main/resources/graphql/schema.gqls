type Page {
    content: [Guess]!
    totalPages: Int
    totalElements: Int
    last: Boolean
    size: Int
    number: Int
    numberOfElements: Int
    first: Boolean
    empty: Boolean
}

type Guess {
    id: ID!
    username: String!
    correct: Boolean!
    letter: String
    exerciseId: Int
    courseId: Int
    courseName: String
    language: String
}

type Mutation {
    dummy: String
}

type Query {
    getGuesses(username: String, page: Int, amount: Int): Page!
}